<Window x:Class="Interop.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Button x:Name="btnClipboard" Content="Get Clipboard" HorizontalAlignment="Left" Margin="65,37,0,0" VerticalAlignment="Top" Width="96" Height="33" Click="btnClipboard_Click"/>
        <TextBox x:Name="txtResult" HorizontalAlignment="Left" Height="59" Margin="65,100,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="166"/>
        <Button x:Name="btnSpace" Content="Get Free Space" HorizontalAlignment="Left" Margin="198,37,0,0" VerticalAlignment="Top" Width="108" Height="33" Click="btnSpace_Click"/>
        <Button x:Name="btnGetText" Content="Get Text" HorizontalAlignment="Left" Margin="341,37,0,0" VerticalAlignment="Top" Width="91" Height="33" Click="btnGetText_Click"/>
        <Button x:Name="btnExcel" Content="Excel" HorizontalAlignment="Left" Margin="341,100,0,0" VerticalAlignment="Top" Width="91" Height="34" Click="btnExcel_Click"/>

    </Grid>
</Window>
